{
  "name": "poppa-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "eslint src --max-warnings 0",
    "biome:check": "biome check .",
    "biome:fix": "biome check --write .",
    "biome:lint": "biome lint .",
    "biome:format": "biome format --write .",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\" \"*.{js,mjs,ts,json,md}\"",
    "format:write": "prettier --write \"src/**/*.{ts,tsx}\" \"*.{js,mjs,ts,json,md}\"",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "validate": "pnpm lint && pnpm typecheck && pnpm test",
    "validate:all": "pnpm lint:strict && pnpm biome:check && pnpm typecheck && pnpm test",
    "prepare": "cd .. && husky poppa_frontend/.husky"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@elevenlabs/react": "^0.12.3",
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-hover-card": "^1.1.1",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@supabase/supabase-js": "^2.45.6",
    "@svgr/webpack": "^8.1.0",
    "@tiptap/extension-placeholder": "^2.6.6",
    "@tiptap/pm": "^2.6.6",
    "@tiptap/react": "^2.6.6",
    "@tiptap/starter-kit": "^2.6.6",
    "anthropic": "^0.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "1.0.0",
    "country-flag-icons": "^1.5.13",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.5.4",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.437.0",
    "micro": "^10.0.1",
    "next": "15.0.5",
    "next-intl": "^3.23.5",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.53.0",
    "react-syntax-highlighter": "^15.5.0",
    "stripe": "^18.1.1",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@shadcn/ui": "^0.0.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.7",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.10",
    "node-mocks-http": "^1.16.2",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "react-syntax-highlighter": "^15.5.0",
    "supertest": "^7.1.1",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.3.4",
    "typescript": "^5"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "biome check --write --no-errors-on-unmatched",
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,mjs,json,md}": [
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@9.6.0"
}
